<%= simple_form_for(@dam) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :descricao %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :radius %>
    
    <%= f.select :basin_id, Basin.all.collect{|p| [p.name,p.id]}%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
